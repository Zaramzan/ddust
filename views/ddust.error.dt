extends ddust.layout

block body
	.container
		.row
			.col-lg-12
				h1.page-header #{error.code}
					small #{error.message}

		.row
			.col-lg-12
				p.lead #{error.message}
				p #{error.debugMessage}
				p Here some helpful links to help you find what you're looking for:
				ul
					li
						a(href='/about') About
					li
						a(href='/blog') Blog
					li
						a(href='/forum') Forum
					li
						a(href='/contact') Contact
